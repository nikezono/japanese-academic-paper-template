{
  "plugins": [
    "latex2e"
  ],
  "filters": {
    "comments": true
  },
  "rules": {
    "preset-ja-engineering-paper": true,
    "preset-japanese": {
      "sentence-length": false
    },
    "preset-ja-engineering-paper": true,
    "preset-ja-technical-writing": {
      "ja-no-mixed-period": {
        "allowPeriodMarks": ["．","："]
      },
      "max-kanji-continuous-len": false,
      "sentence-length": false
    },
    "@proofdict/proofdict": {
      "dictURL": "https://azu.github.io/proof-dictionary/"
    },
    "prh": {
      "rulePaths": [
        "test/prh.yml"
      ]
    }
  }
}
